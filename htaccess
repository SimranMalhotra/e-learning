//Rewrite to www
Options +FollowSymLinks
RewriteEngine on
RewriteCond %{HTTP_HOST} ^jntukelearn.in[nc]
RewriteRule ^(.*)$ http://www.jntukelearn.in/$1 [r=301,nc]

//Caching schema
<FilesMatch "\.(jpg|png|css|js)$">
Header set Cache-Control "private, proxy-revalidate, max-age=72576000"
</FilesMatch>

//Custom 404 errors
ErrorDocument 404 404.html

//Change default directory page
DirectoryIndex index.html

//Prevent viewing of .htaccess file
<Files .htaccess>
order allow,deny
deny from all
</Files>

//Prevent directory listings
Options All -Indexes
 